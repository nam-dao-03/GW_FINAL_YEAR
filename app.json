{
  "expo": {
    "name": "NutriHeart",
    "slug": "nutrition_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/imgs/app-icon.png",
      "imageWidth": 200,
      "resizeMode": "contain",
      "backgroundColor": "#F8F0E3"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/imgs/app-icon.png",
        "backgroundColor": "#F8F0E3"
      },
      "package": "com.namdao994.nutrition_app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      "expo-sqlite",
      [
        "expo-notifications",
        {
          "icon": "./assets/imgs/app-icon.png",
          "color": "#F8F0E3",
          "defaultChannel": "default",
          "sounds": [
            "./assets/sounds/happy_bells_notification.wav"
          ],
          "enableBackgroundRemoteNotifications": false
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#F8F0E3",
          "image": "./assets/imgs/app-icon.png",
          "dark": {
            "image": "./assets/imgs/app-icon.png",
            "backgroundColor": "#F8F0E3"
          }
        }
      ],
      [
        "@react-native-community/datetimepicker",
        {
          "android": {
            "datePicker": {
              "colorAccent": {
                "light": "#3ccd8a"
              },
              "colorControlActivated": {
                "light": "#3ccd8a",
                "dark": "#3ccd8a"
              }
            },
            "timePicker": {
              "headerBackground": {
                "light": "#3ccd8a",
                "dark": "#3ccd8a"
              },
              "numbersSelectorColor": {
                "light": "#3ccd8a",
                "dark": "#3ccd8a"
              }
            }
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow CalBuddy to access your camera",
          "microphonePermission": "Allow CalBuddy to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "3d04202f-52a0-4c38-9a16-d3f693d004af"
      }
    },
    "owner": "namdnstudent"
  }
}
